import mx.screens.Form;class ServerTimeForm extends Form {		static var symbolOwner:Object = ServerTimeForm;	static var symbolName:String = "ServerTimeForm";     	var className:String = "ServerTimeForm";		/* declare a reference to the 	serverTime_wsc Web Service Connector */	private var serverTime_wsc; 		function onLoad() {		/* when the form loads onto the screen		trigger the Web Service Connector */		serverTime_wsc.trigger();	}}